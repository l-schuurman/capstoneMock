{
  "name": "@teamd/database",
  "version": "1.0.2",
  "description": "Team D database schema overlay and local extensions",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "registry": "http://localhost:4873"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "generate": "node scripts/generate-migration.js",
    "generate:simple": "drizzle-kit generate",
    "migrate": "tsx src/migrate.ts",
    "seed": "tsx src/seed.ts",
    "push": "drizzle-kit push",
    "studio": "drizzle-kit studio",
    "promote": "node src/scripts/promote-schema.js",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@large-event/database": "^1.0.1",
    "drizzle-orm": "^0.30.10",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@types/node": "^24.10.0",
    "@types/pg": "^8.15.6",
    "drizzle-kit": "^0.21.4",
    "tsup": "^8.3.5",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./overlays": {
      "types": "./dist/overlays/index.d.ts",
      "default": "./dist/overlays/index.js"
    }
  }
}
