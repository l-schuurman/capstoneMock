# Team D API Environment Variables
#
# NOTE: Default ports and URLs are defined in teamd.config.ts
# Override them here only if you need different values

# Server Configuration (optional - defaults from teamd.config.ts)
# PORT=3004
# HOST=0.0.0.0
NODE_ENV=development

# Database (optional - defaults from teamd.config.ts)
# DATABASE_URL=postgresql://postgres:postgres@localhost:5432/large_event

# Redis (optional - defaults from teamd.config.ts)
# REDIS_URL=redis://localhost:6379

# JWT Authentication (optional - defaults from teamd.config.ts)
# JWT_SECRET=your-secret-key-here-change-in-production

# Other Team APIs (optional - defaults from teamd.config.ts)
# TEAM_A_API_URL=http://localhost:3001
# TEAM_B_API_URL=http://localhost:3002
# TEAM_C_API_URL=http://localhost:3003

# Docker flag (set to 'true' when running in Docker)
# DOCKER=false
